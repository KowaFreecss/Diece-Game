#include <stdio.h>
#include <stdlib.h>

int main{
    int a,b,name;
    
    srand(time(NULL));
    
    a=rand()%6+1;
    b=rand()%6+1;
    printf("What is your name? \n");
    scanf("%s",name);
    printf("Hello, %s!",name);
    
    printf("Rolling the diece...\nDie1:%d \nDie2%d",a,b);
    
    printf("Total value:%d\n",a+b);
    
    if(a+b>=7){
    printf("You, won!!");
    }else{
    printf("You, lost");
    } 
    
    return 0;
}
